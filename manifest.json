{
  "manifest_version": 3,
  "name": "CTBids Seller Image Helper",
  "version": "1.2.1.0",
  "description": "Downloads photos from the CTBids seller dashboard, using item titles as filenames.",
  "permissions": ["activeTab", "scripting", "downloads", "offscreen"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  }
}
